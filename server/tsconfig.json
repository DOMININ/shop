{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "lib": ["dom", "esnext"],
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "outDir": "./dist"
    },
    "include": ["server/**/*.ts", "server/**/*.tsx", "src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules"]
}